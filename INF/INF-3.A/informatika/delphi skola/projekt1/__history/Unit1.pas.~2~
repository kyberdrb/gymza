unit Unit1;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs, StdCtrls, ExtCtrls;

type
  TForm1 = class(TForm)
    Button1: TButton;
    Image1: TImage;
    procedure Button1Click(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Form1: TForm1;

implementation

{$R *.dfm}

procedure TForm1.Button1Click(Sender: TObject);
begin


 Image1.Canvas.Rectangle(100, 100, 200, 200);
 Image1.Canvas.Ellipse(125, 50, 175, 100);
 Image1.Canvas.Rectangle(0, 100, 100, 133);
 Image1.Canvas.Rectangle(200, 100, 300, 133);
 Image1.Canvas.Rectangle(105, 0, 100, 133);
 Image1.Canvas.Rectangle(200, 100, 300, 133);


end;

end.
